{
  "type": "module",
  "name": "happy_testing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack",
    "start": "next start",
    "lint": "eslint",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:api": "cypress run --spec 'cypress/e2e/api/**/*.cy.js'",
    "cypress:api:open": "cypress open --config specPattern='cypress/e2e/api/**/*.cy.js'",
    "cypress:api:record": "cypress run --spec 'cypress/e2e/api/**/*.cy.js' --record",
    "cypress:e2e": "cypress run --spec 'cypress/e2e/tests/**/*.cy.js'",
    "cypress:e2e:record": "cypress run --spec 'cypress/e2e/tests/**/*.cy.js' --record",
    "cypress:record": "cypress run --record",
    "playwright:test": "playwright test",
    "playwright:open": "playwright test --ui",
    "playwright:codegen": "playwright codegen",
    "test:api": "playwright test --config=api-tests/config/api.config.ts",
    "test:api:ui": "playwright test --config=api-tests/config/api.config.ts --ui",
    "test:api:debug": "playwright test --config=api-tests/config/api.config.ts --debug"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.22.0",
    "@prisma/client": "^6.16.2",
    "bcryptjs": "^3.0.2",
    "next": "15.5.4",
    "prisma": "^6.16.2",
    "react": "19.1.0",
    "react-dom": "19.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cypress": "^15.6.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.4",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "wait-on": "^8.0.1"
  }
}
